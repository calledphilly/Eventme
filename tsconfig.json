{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react-native",
		"target": "esnext",
		"module": "commonjs",
		"lib": ["es2015", "dom"],
		"strict": true,
		"esModuleInterop": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
